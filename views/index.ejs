.wine-slider {
  position:relative;
  max-width:1260px;
  margin:50px auto;
  .slide-container {
    position:relative;
    height:420px;
    left:50%;
    margin-left:-90px;
    transition:transform 0.2s;
    a[class*='slide'] {
      float:left;
      display:block;
      height:340px;
      width:350px;
      background-size:cover;
      background-position:center;
      margin:40px;
      filter: blur(5px);
      transition:all 0.2s;
      &.active {
        filter: blur(0px);
        transform:scale(1.2);
      }
    }
  }
  .arrow {
    position:absolute;
    top:50%;
    font-size:40px;
    font-weight:bold;
    cursor:pointer;
    &.previous {
      left:0px;
    }
    &.next {
      right:0px;
    }
  }
}

.aboutSection {
  padding-bottom: 200px;
  position: relative;
  overflow: hidden;
  min-height: 100vh;
  justify-content: center;
  align-items: center;
}

.sliderSection {
  display: flex;
  min-width: 100%;
}

.about {
  display: flex;
  align-items: center;
  justify-content: center;
  padding-top: 3rem;
}

.aboutImg {
