    <script>
        var slideIndex = 1;
        showDivs(slideIndex);

        function plusDivs(n) {
            showDivs(slideIndex += n);
        }

        function showDivs(n) {
            var i;
            var x = document.getElementsByClassName("mySlides");
            if (n > x.length) {slideIndex = 1}
            if (n < 1) {slideIndex = x.length}
            for (i = 0; i < x.length; i++) {
                x[i].className = x[i].className.replace(" show", "");
            }
            x[slideIndex-1].className += " show";
        }

        setInterval(function() {
            plusDivs(1);
        }, 100000);

var slidePosition = 0,
    numOfSlide = $(".slide").length,
    currentSlide = Math.floor(numOfSlide / 2),
    slideWidth = $(".slide").outerWidth(true);

moveSlide(currentSlide);
$(".slide-" + currentSlide).addClass("active");

$(".slide-container").css("width", numOfSlide * slideWidth);

$(".previous").click(function(){
  
  $(".slide-" + currentSlide).removeClass("active");
  if ((currentSlide - 1) >= 0) {
    currentSlide--;
  } else {
    currentSlide = (numOfSlide - 1);
  }
  $(".slide-" + currentSlide).addClass("active");
  
  moveSlide(currentSlide);
  
});

$(".next").click(function(){
  
  $(".slide-" + currentSlide).removeClass("active");
  if ((currentSlide + 1) < numOfSlide) {
    currentSlide++;
  } else {
    currentSlide = 0;
  }
  $(".slide-" + currentSlide).addClass("active");
  
  moveSlide(currentSlide);
  
});

function moveSlide(slideNumber) {
  var slidePosition = -1 * (slideNumber * slideWidth);
  $(".slide-container").css({"transform":"translateX("+ slidePosition +"px)"});
}



console.clear();

gsap.registerPlugin(ScrollTrigger);
const slides = gsap.utils.toArray(".about");
const tl = gsap.timeline({
 scrollTrigger: {
    trigger: ".sliderSection",
    start: "top top",
    end: "+=" + (100 * (slides.length - 1)) + "5",
    pin: ".sliderSection",
    scrub: true,
    markers: false, // Disable markers
 }
}).to(slides, {
 xPercent: -(100 * (slides.length - 1)),
 ease: "none",
});

const images = gsap.utils.toArray(".aboutImg");
images.forEach((image, index) => {
 gsap.from(image, {
    opacity: 0.2,
    scale: 0.5,
    transformOrigin: "center center",
    ease: "none",
    scrollTrigger: {
      trigger: slides[index],
      containerAnimation: tl,
      start: "left 75%",
      end: "left 25%",
      markers: false, // Disable markers for each image as well
      scrub: true,
    }
 });
});

tl.progress(1).progress(0);


// Pricing table - mobile only slider
var init = false;
var pricingCardSwiper;
function swiperCard() {
  if (window.innerWidth <= 991) {
    if (!init) {
      init = true;
      pricingCardSwiper = new Swiper(".cardSlider", {
        slidesPerView: "1",
        spaceBetween: 0,
        grabCursor: true,
        keyboard: true,
        autoHeight: false,
        navigation: {
          nextEl: '.swiper-button-next',
          prevEl: '.swiper-button-prev',
        },
        pagination: {
          el: '.swiper-pagination',
          clickable: true,
        },
        breakpoints: {
          768: {
            slidesPerView: 2,
          }
        },
      });
    }
  } else if (init) {
    pricingCardSwiper.destroy();
    init = false;
  }
}
swiperCard();
window.addEventListener("resize", swiperCard);

    </script>
 
